#!/bin/bash

#noob
cd
wget https://raw.githubusercontent.com/Infotricky/Script.menu/master/userlist
mv ./user-list /usr/local/bin/user-list
chmod +x /usr/local/bin/user-list

# instal Buat Akun SSH/OpenVPN
cd
wget https://raw.githubusercontent.com/Infotricky/Script.menu/master/buatakaun
mv ./buatakun /usr/bin/buatakun
chmod +x /usr/bin/buatakun

# instal Generate Akun SSH/OpenVPN
cd
wget https://raw.githubusercontent.com/Infotricky/Script.menu/master/generateAkaun
mv ./generate /usr/bin/generateAkaun
chmod +x /usr/bin/generateAkaun

# instal Generate Akun Trial
cd
wget https://raw.githubusercontent.com/Infotricky/Script.menu/master/akaunTrial
mv ./trial /usr/bin/akaunTrial
chmod +x /usr/bin/akaunTrial

# instal  Ganti Password Akun SSH/VPN
cd
wget https://raw.githubusercontent.com/Infotricky/Script.menu/master/userpass
mv ./userpass /usr/bin/userpass
chmod +x /usr/bin/userpass

# instal Generate Akun SSH/OpenVPN
cd
wget https://raw.githubusercontent.com/Infotricky/Script.menu/master/userrenew
mv ./userrenew /usr/bin/userrenew
chmod +x /usr/bin/userrenew

# instal Hapus Akun SSH/OpenVPN
cd
wget https://raw.githubusercontent.com/Infotricky/Script.menu/master/userdelete
mv ./userdelete /usr/bin/userdelete
chmod +x /usr/bin/Userdelete


# instal Cek Login Dropbear, OpenSSH & OpenVPN
cd
wget https://raw.githubusercontent.com/Infotricky/Script.menu/master/userlogin
mv ./userlogin /usr/bin/userlogin
chmod +x /usr/bin/userlogin

# instal Auto Limit Multi Login
cd
wget https://raw.githubusercontent.com/Infotricky/Script.menu/master/autolimit
mv ./autolimit /usr/bin/autolimit
chmod +x /usr/bin/autolimit

# instal Auto Limit Script Multi Login
cd
wget https://raw.githubusercontent.com/Infotricky/Script.menu/master/autolimituser
mv ./auto-limit-script /usr/bin/autolimituser
chmod +x /usr/bin/autolimituser

# instal Melihat detail user SSH & OpenVPN 
cd
wget https://raw.githubusercontent.com/Infotricky/Script.menu/master/userdetail
mv ./userdetail /usr/bin/userdetail
chmod +x /usr/bin/userdetail

# instal Delete Akun Expire
cd
wget https://raw.githubusercontent.com/Infotricky/Script.menu/master/deleteuserexpire
mv ./deleteuserexpire /usr/bin/deleteuserexpire
chmod +x /usr/bin/deleteuserexpire

# instal  Kill Multi Login
cd
wget https://raw.githubusercontent.com/Infotricky/Script.menu/master/autokilluser
mv ./autokilluser /usr/bin/autokilluser
chmod +x /usr/bin/autokilluser

# instal Auto Banned Akun
cd
wget https://raw.githubusercontent.com/Infotricky/Script.menu/master/userban
mv ./userban /usr/bin/userban
chmod +x /usr/bin/userban

# instal Unbanned Akun
cd
wget https://raw.githubusercontent.com/Infotricky/Script.menu/master/userunban
mv ./userunban /usr/bin/userunban
chmod +x /usr/bin/userunban

# instal Mengunci Akun SSH & OpenVPN
cd
wget https://raw.githubusercontent.com/Infotricky/Script.menu/master/userlock
mv ./userlock /usr/bin/userlock
chmod +x /usr/bin/userlock

# instal Membuka user SSH & OpenVPN yang terkunci
cd
wget https://raw.githubusercontent.com/Infotricky/Script.menu/master/userunlock
mv ./userunlock /usr/bin/userunlock
chmod +x /usr/bin/userunlock

# instal Melihat daftar user yang terkick oleh perintah user-limit
cd
wget https://raw.githubusercontent.com/Infotricky/Script.menu/master/loglimit
mv ./loglimit /usr/bin/loglimit
chmod +x /usr/bin/loglimit

# instal Melihat daftar user yang terbanned oleh perintah user-ban
cd
wget https://raw.githubusercontent.com/Infotricky/Script.menu/master/logban
mv ./logban /usr/bin/logban
chmod +x /usr/bin/logban

# instal Buat Akun PPTP VPN
cd
wget https://raw.githubusercontent.com/Infotricky/Script.menu/master/useradpptp
mv ./useraddpptp /usr/bin/useradpptp
chmod +x /usr/bin/useradpptp

# instal Hapus Akun PPTP VPN
cd
wget https://raw.githubusercontent.com/Infotricky/Script.menu/master/userdeletepptp
mv ./userdeletepptp /usr/bin/userdeletepptp
chmod +x /usr/bin/userdeletepptp

# instal Lihat Detail Akun PPTP VPN
cd
wget https://raw.githubusercontent.com/Infotricky/Script.menu/master/userdetailpptp
mv ./detailpptp /usr/bin/userdetailpptp
chmod +x /usr/bin/userdetailpptp

# instal Cek login PPTP VPN
cd
wget https://raw.githubusercontent.com/Infotricky/Script.menu/master/userdetailpptp
mv ./userloginpptp /usr/bin/userdetailpptp
chmod +x /usr/bin/userdetailpptp

# instal Lihat Daftar User PPTP VPN
cd
wget https://raw.githubusercontent.com/Infotricky/Script.menu/master/alluserpptp
mv ./alluserpptp /usr/bin/alluserpptp
chmod +x /usr/bin/alluserpptp

# instal Set Auto Reboot
cd
wget https://raw.githubusercontent.com/Infotricky/Script.menu/master/autoreboot
mv ./autoreboot /usr/bin/autoreboot
chmod +x /usr/bin/autoreboot

# Install SPEED tES
apt-get install python
wget https://raw.githubusercontent.com/Infotricky/Script.menu/master/speedtest.py
chmod +x speedtest.py

# instal userdelete
cd
wget https://raw.githubusercontent.com/Infotricky/Script.menu/master/deleteuser
mv ./userdelete /usr/bin/deleteuser
chmod +x /usr/bin/deleteuser

# Install Menu
cd
wget https://raw.githubusercontent.com/redhatz91/student.pack/master/menu
mv ./menu /usr/local/bin/menu
chmod +x /usr/local/bin/menu

rm -f /root/iplist.txt

rm -f /root/update

rm -f /root/debian7latest.sh

rm -f /root/debian7latest.sh.x

rm -f /root/ddos-deflate-master.zip

rm -f /root/.bash_history && history -c 
